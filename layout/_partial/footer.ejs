<footer class="footer">
  <div class="outer">
    <ul>
      <% if (theme.icp&&theme.icp.enable){ %>
        <li>
          <a href="<%= theme.icp.url %>" target="_black"><%= theme.icp.text %></a>
        </li>
      <% } %>
    </ul>
    <ul>
      <li>
        &copy;
        <% if (theme.since && !isNaN(theme.since) && theme.since < date(new Date(), 'YYYY')) { %>
          <%- theme.since%>-<% } %>
          <%= date(new Date(), 'YYYY') %>
        <%= theme.studio %>
        <%= theme.footer_copyright %>
      </li>
    </ul>
  </div>
</footer>